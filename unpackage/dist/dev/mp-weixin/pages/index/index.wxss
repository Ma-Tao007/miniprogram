.weui-cell{
	padding: 16rpx 15px;
	position: relative;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	align-items: center;
	margin: auto 0px;
}
.weui-cell__bd{
	width: 50%;
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	flex: 1;
}
.weui-input{
	width: 100%;
	border: 0;
	outline: 0;
	-webkit-appearance: none;
	background-color: transparent;
	font-size: inherit;
	color: inherit;
	height: 1.47058824em;
	line-height: 1.47058824;
}
/* 记住密码 */
.repass{
	color: #888888;
}
/* 忘记密码 */
.forgetPass{
	color: #888888;
	float: right;
}
